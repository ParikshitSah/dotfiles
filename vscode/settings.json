{
    "workbench.editorAssociations": {
        "*.copilotmd": "vscode.markdown.preview.editor"
    },
    "vscode-office.editorTheme": "Github Dark",
    "files.autoSave": "afterDelay",
    "editor.fontFamily": "monospace, Agave Nerd Font, JetBrains Mono",
    "editor.multiCursorModifier": "ctrlCmd",
    "workbench.colorTheme": "Catppuccin Frapp√©",
    "terminal.integrated.inheritEnv": false,
    "vim.handleKeys": {
        "<C-j>": false,
        "<C-d>": true,
        "<C-s>": false,
        "<C-z>": false
    },
    "application.shellEnvironmentResolutionTimeout": 15,
    "editor.lineNumbers": "relative",
    "workbench.iconTheme": "catppuccin-frappe",
    "catppuccin.accentColor": "rosewater",
    "raspberry-pi-pico.python3Path": "/usr/bin/python3",
    "github.copilot.enable": {
        "*": false,
        "plaintext": false,
        "markdown": false,
        "scminput": false
    },
    "window.confirmSaveUntitledWorkspace": false,
    "[typescriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[css]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[html]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "telemetry.telemetryLevel": "off",
    "docker.lsp.telemetry": "off"
}